{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/api/*.ts"],
  "spec": {
    "outputDirectory": "tsoa-build",
    "specVersion": 3,
    "description": "API for Smart Grocery List.",
    "securityDefinitions": {
      "local_session": {
        "type": "localSession",
        "name": "local_session"
      }
    },
    "rootSecurity": [{ "local_session": [""] }]
  },
  "routes": {
    "basePath": "/api",
    "routesDir": "tsoa-build",
    "authenticationModule": "./src/utils/tsoaAuthentication.ts"
  },
  "swagger": {
    "info": {
      "description": "API for Smart Grocery List.",
      "title": "Smart Grocery List API"
    },
    "servers": [
      {
        "url": "/api",
        "description": "The main API."
      }
    ]
  }
}
